/**
 * Implements hook_icon_providers().
 */
function fontawesome_icons_icon_providers() {
  $providers['fontawesome_icons'] = array(
    'title' => t('Font-Awesome'),
    'url' => 'http://fortawesome.github.io/Font-Awesome/',
  );
  return $providers;
}

